{
  "name": "cordova-plugin-fonts",
  "version": "1.0.0",
  "description": "Cordova Fonts plugin",
  "keywords": [
    "cordova",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-browser",
    "cordova-electron",
    "plugin",
    "fonts"
  ],
  "homepage": "https://github.com/adapt-it/cordova-fonts",
  "bugs": {
    "url": "https://github.com/adapt-it/cordova-fonts/issues"
  },
  "license": "Apache-2.0",
  "cordova": {
    "id": "cordova-plugin-fonts",
    "platforms": [
        "android",
        "browser",
        "electron",
        "ios"
    ]
  },
  "main": "fonts.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "test": "npm run jshint",
    "jshint": "node node_modules/jshint/bin/jshint www && node node_modules/jshint/bin/jshint src && node node_modules/jshint/bin/jshint tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adapt-it/cordova-fonts.git"
  },
  "author": "Erik Brommers",
  "engines": {
    "cordovaDependencies": {
      "1.0.0": { "cordova": ">11.0.0" }
    }  
  },
  "devDependencies": {
    "jshint": "^2.6.0"  
  }
}
